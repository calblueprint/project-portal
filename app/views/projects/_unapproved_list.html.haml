%h4.details-title
  Public Projects
-if public_projects.empty?
  No Public Projects
-public_projects.each do |p|
  =render :partial=>"shared/project_card", :locals => {:project => p}
=will_paginate public_projects, :param_name => 'public_page'

%h4.details-title
  Private Projects
-if private_projects.empty?
  No Private Projects
-private_projects.each do |p|
  =render :partial=>"shared/project_card", :locals => {:project => p}
=will_paginate private_projects, :param_name => 'private_page'

